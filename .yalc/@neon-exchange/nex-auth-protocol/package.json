{
  "name": "@neon-exchange/nex-auth-protocol",
  "version": "1.7.42-48a1dfe9",
  "description": "Implementation of NEX Auth Protocol",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "sideEffects": false,
  "repository": "https://github.com/neonexchange/nex-auth-protocol",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "yarn clean && yarn build:main && yarn build:module",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "yarn fix:prettier && yarn fix:tslint",
    "fix:prettier": "prettier \"src/**/*.{ts,json}\" --write --config .prettierrc",
    "fix:tslint": "tslint --fix --project .",
    "test": "yarn build && yarn test:tests-only",
    "test:tests-only": "yarn test:lint && yarn test:tsc && yarn test:unit",
    "test:lint": "tslint --project .",
    "test:tsc": "tsc -p tsconfig.json --noEmit",
    "test:unit": "nyc --silent jest",
    "test:unit:coverage": "jest --verbose --coverage",
    "watch": "yarn clean && yarn test:unit --watch",
    "cov": "yarn build && yarn test:unit && yarn cov:html",
    "cov:html": "nyc report --reporter=html",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && yarn",
    "clean": "rm -rf build test",
    "all": "yarn reset && yarn test && yarn cov:check",
    "prepare-release": "yarn all && yarn version",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('nex-auth-protocol must be installed with Yarn: https://yarnpkg.com/')\""
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "bip39": "2.5.0",
    "browserify-aes": "1.2.0",
    "crypto-js": "3.1.9-1",
    "elliptic": "6.4.1",
    "futoin-hkdf": "1.0.2",
    "lodash": "4.17.11",
    "randombytes": "2.1.0",
    "scrypt-js": "2.0.4"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
